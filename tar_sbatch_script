#!/bin/bash

export SLURM_EXPORT_ENV=ALL
echo "======================================================"
echo "Start: $(date)"
echo "======================================================"

echo "======================================================"
echo "lscpu:"
lscpu
echo "======================================================"

echo "sbatch command:"
echo $slurmcommand
echo "======================================================"

echo "XZ_OPT = $XZ_OPT"
echo "tar command:"
echo "$tarcommand"
echo "======================================================"

$tarcommand

echo "======================================================"
echo "End: $(date)"
echo "======================================================"